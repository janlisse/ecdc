{
  "family":"abcFamily",
  "containerDefinitions":[
    {
      "name":"abcName",
      "image":"quay.io/imageName:latest",
      "cpu":5,
      "memory":1024,
      "links":[
        "link"
      ],
      "portMappings":[
        {
          "containerPort":80,
          "hostPort":9000,
          "protocol":"udp"
        }
      ],
      "logConfiguration":{
        "logDriver":"awslogs",
        "options":{
          "awslogs-group":"awslogs-test",
          "awslogs-region":"eu-west-1"
        }
      },
      "essential":false,
      "entryPoint":[
        "/bin/entrypoint"
      ],
      "command":[
        "cmd"
      ],
      "environment":[
        {
          "name":"PATH",
          "value":"/bin"
        }
      ],
      "mountPoints":[
        {
          "sourceVolume":"/source",
          "containerPath":"/container",
          "readOnly":true
        }
      ],
      "ulimits":[
        {
          "name":"nofile",
          "softLimit":4096,
          "hardLimit":8192
        }
      ],
      "volumesFrom":[
        {
          "sourceContainer":"container",
          "readOnly":true
        }
      ]
    },
    {
      "name":"xyzName",
      "image":"quay.io/otherImageName:1.2.3",
      "cpu":10,
      "memory":512,
      "links":[
        "other-link"
      ],
      "portMappings":[
        {
          "containerPort":80,
          "hostPort":8000,
          "protocol":"tcp"
        }
      ],
      "essential":false,
      "entryPoint":[
        "/bin/entrypoint"
      ],
      "command":[
        "arg1",
        "arg2"
      ],
      "mountPoints":[
        {
          "sourceVolume":"/source",
          "containerPath":"/container",
          "readOnly":true
        }
      ],
      "ulimits":[
        {
          "name":"nofile",
          "softLimit":4096,
          "hardLimit":8192
        }
      ],
      "volumesFrom":[
        {
          "sourceContainer":"container",
          "readOnly":true
        }
      ]
    }
  ],
  "volumes":[
    {
      "name":"volumeName",
      "host":{
        "sourcePath":"hostSourcePath"
      }
    }
  ],
  "taskRoleArn":"arn:aws:iam::xxxxxxxxxxxx:role/test-task-role-arn"
}
